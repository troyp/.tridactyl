" ╭─────────╮
" │ General │
" ╰─────────╯

" general libraries
jsb -r S.js
jsb -r js/sprintf.js
" tridactyl-specific libraries
jsb -r utils.js
" app-specific libraries
jsb -r js/suncalc.js
" app-specific code
jsb -r apps.js
" site-specific code
jsb -r sites.js
source ~/.tridactyl/sites.tri
" personal info
jsb -r personal/personal.js

jsb window.S = S
jsb window.utils = utils
jsb window.apps = apps
jsb window.sites = sites
jsb window.sprintf = sprintf

