autocmd DocStart ^https?://medium\.com(/|$) urlmodify -r https?://medium\.com/? https://scribe.rip/

" ───────────────────────────────────────────────────────────────────────────────
" ╭────────────────╮
" │ PrimeVideo.com │
" ╰────────────────╯
command: primevbind "Bind key for www.primevideo.com" bindurl https://www.primevideo.com
command: primevbind: "Bind key with description for www.primevideo.com" docbindurl https://www.primevideo.com
primevbind c<C-h> showkeysurl https://www.primevideo.com/ c
primevbind: cc "Show episode" cujs fillcmdline_nofocus($1("div.contentTitlePanel>div.subtitle").textContent)
primevbind: ct "Show episode" cujs fillcmdline_nofocus($1("div.contentTitlePanel>div.title").textContent)
primevbind: cyy "Show episode" cujs cutils.yankWithMsg($1("div.contentTitlePanel>div.subtitle").textContent)
primevbind: cyt "Show episode" cujs cutils.yankWithMsg($1("div.contentTitlePanel>div.title").textContent)

