" ╭─────────────╮
" │ RamdaJS.com │
" ╰─────────────╯
commdoc ramdafiltercat "Filter methods by category"
command ramdafiltercat csites -r S.js ; js -p csites.ramda.filterCategory(S.capitalize(JS_ARG));

bindurl ramdajs.com/ c<C-h> showkeysurl ramdajs.com/ c
bindurl: ramdajs.com/ cc "Filter methods by category" fillcmdline ramdafiltercat

" ───────────────────────────────────────────────────────────────────────────────
" ╭────────────╮
" │ Reddit.com │
" ╰────────────╯
bindurl reddit.com/ c<C-h> showkeysurl reddit.com/ c
bindurl reddit.com/ cto urltoggle https://www.reddit.com https://old.reddit.com

" ───────────────────────────────────────────────────────────────────────────────
" ╭───────────────────╮
" │ StackExchange.com │
" ╰───────────────────╯
command se_js composite cujs -r sites/se.c.js ; js
commdoc sebind "Bind key for stackexchange sites"
command sebind jsb -d¦ sites.se.bind("--mode=normal", ...JS_ARGS)¦
commdoc sebind: "Bind key with description for stackexchange sites"
command sebind: jsb -d¦ sites.se.bind_("--mode=normal", ...JS_ARGS)¦
command: seexpandcomments "Expand all comments" cujs cutils.click("a.js-show-link", {all:true});

sebind: cc "Expand all comments" seexpandcomments
sebind: cC "Cleanup (remove clutter)" se_js se.cleanup()

" | ct -- SE/toggle |
sebind: ctw "Toggle widen content" se_js se.widen()

" ───────────────────────────────────────────────────────────────────────────────
" ╭──────────────╮
" │ TVTropes.org │
" ╰──────────────╯
bindurl: tvtropes.org cc "Toggle spoilers" csites csites.tvtropes.spoilers()

" ───────────────────────────────────────────────────────────────────────────────
" ╭───────────────╮
" │ Wikipedia.org │
" ╰───────────────╯
commdoc wpbind "Bind key for wikipedia.org"
command wpbind bindurl https://([a-z]+\.)?(wikipedia|wikisource|wiktionary|wikibooks|wikimedia)\.org
commdoc wpbind: "Bind key with description for wikipedia.org"
command wpbind: bindurl: https://([a-z]+\.)?(wikipedia|wikisource|wiktionary|wikibooks|wikimedia)\.org
command: wpbdoc "Provide a description on a separate line" js ;

wpbind c<C-h> showkeysurl wikipedia.org c
wpbdoc cd "Open page in Wiktionary"
wpbind cd jsb utils.tab.open(`https://en.wiktionary.org/w/index.php?search=${sites.wp.getTopic()}`, "here")
wpbdoc cD "Open page in Wiktionary (adjacent tab)"
wpbind cD jsb utils.tab.open(`https://en.wiktionary.org/w/index.php?search=${sites.wp.getTopic()}`, "related")
wpbdoc ce "Open page in Wikipedia"
wpbind ce jsb utils.tab.open(`https://en.wikipedia.org/w/index.php?search=${sites.wp.getTopic()}`, "here")
wpbdoc cE "Open page in Wikipedia (adjacent tab)"
wpbind cE jsb utils.tab.open(`https://en.wikipedia.org/w/index.php?search=${sites.wp.getTopic()}`, "related")

" | ct -- wikipedia/toggle |
wpbind ct<C-h> showkeysurl wikipedia.org ct
wpbind: ctm "Toggle mobile version" urltoggle https://en.m https://en
wpbind: ctj "Toggle japanese edition" csites csites.wp.toggleJa()

wpbind: cC "Cleanup (remove clutter from page)" csites csites.wp.cleanup()
wpbind: c;; "Search ENGINES with page title" jsb fillcmdline(`searchwithengines ${sites.wp.getTopic()}`)

" ───────────────────────────────────────────────────────────────────────────────
" ╭───────────────────╮
" │ Woolworths.com.au │
" ╰───────────────────╯
command: woolbind "Bind key for woolworths.com.au" bindurl https://www.woolworths.com.au/
command: woolbind: "Bind key with description for woolworths.com.au" bindurl: https://www.woolworths.com.au/
command: woolbdoc "Provide a description on a separate line" js ;

commdoc woolsearch "Search woolworths"
command woolsearch jsb -d¦ sites.wool.search(JS_ARGS, "here")¦
commdoc woolsearch_adj "Search woolworths (adjacent tab)"
command woolsearch_adj jsb -d¦ sites.wool.search(JS_ARGS, "related")¦

woolbind c<C-h> showkeysurl https://www.woolworths.com.au/ c

" | co -- woolworths/open |
woolbind co<C-h> showkeysurl https://www.woolworths.com.au/ co
bdoc coc "Goto cart"
woolbind coc cujs if (!cutils.click("button.close-button")) cutils.click("#header-checkout-cart-button")
woolbind: coC "Goto checkout" openorsummon https://www.woolworths.com.au/shop/checkout
woolbind: coo "Goto orders" openorsummon https://www.woolworths.com.au/shop/myaccount/myorders
woolbind: cot "Open tracking in Chrome" jsb sites.wool.openTracking();

" | cos -- woolworths/open/specials |
woolbind cos<C-h> showkeysurl https://www.woolworths.com.au/ cos
woolbdoc cosp "Goto pantry specials"
woolbind cosp openorsummon https://www.woolworths.com.au/shop/browse/pantry/pantry-specials?pageNumber=1
woolbind: cosh "Goto half-price specials" openorsummon https://www.woolworths.com.au/shop/browse/specials/half-price
woolbind: coss "Goto specials" openorsummon "https://www.woolworths.com.au/shop/browse/specials"

woolbdoc cn "Set no substitutions message for each item"
woolbind cn cujs $$("input[type=checkbox]", $1("wow-checkout-cart")).forEach(e=> e.checked && e.click())

" | cs -- woolworths/search |
woolbind cs<C-h> showkeysurl https://www.woolworths.com.au/ cs
woolbind: css "Search products in current tab" fillcmdline woolsearch
woolbind: csa "Search products in adjacent tab" fillcmdline woolsearch_adj

" | cx -- woolworths/miscellaneous |
woolbind cx<C-h> showkeysurl https://www.woolworths.com.au/ cx
woolbdoc cxh "Show only half-price products"
woolbind cxh cujs $$(".product-grid--tile").forEach(e=>{!$1('img[altt"Half Price"]', e) && e.remove()});
